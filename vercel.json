{
    "builds": [{
        "src": "recipes/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/recipeapp/",
        "config": { "distDir": "recipeapp/" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },       
        {
            "src": "/(.*)",
            "dest": "recipes/wsgi.py"
        }
    ]
}







